<node>
	<!--
		org.ofono.Siri:

		Object path: [variable prefix]/{modem0,modem1,...}
	-->
	<interface name="org.ofono.Siri">

		<!--
			Enabled:

			This property indicates whether Siri is available on
			the device.
		-->
		<property name="Enabled" type="b" access="read" />

		<!--
			EyesFreeMode:

			This property indicates the state of Siri Eyes Free
			Mode.  The current possible values are: "enabled"
			and "disabled"
		-->
		<property name="EyesFreeMode" type="s" access="readwrite" />

	</interface>
</node>
